{
  "version": "0.2",
  "language": "en,de",
  "allowCompoundWords": true,
  "dictionaries": ["en_US", "de-de", "typescript", "node", "npm", "html", "css", "software-terms"],
  "ignorePaths": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/.output/**",
    "**/.next/**",
    "**/coverage/**",
    "**/*.code-workspace",
    "*.code-workspace",
    "**/*.lock",
    "**/*.{svg,png,jpg,jpeg,ico,woff*,ttf}"
  ],
  "languageSettings": [
    {
      "languageId": "markdown",
      "locale": "en,de",
      "caseSensitive": false
    },
    {
      "languageId": "*",
      "locale": "en,de",
      "caseSensitive": false
    }
  ],
  "ignoreRegExpList": [
    "/\\b[A-Z]{2,}\\b/g",
    "/\\b\\d+\\b/g",
    "/_kabel\\.htm/gi",
    "/href\\*=[\"']_kabel/gi",
    "/\\[href\\*=[\"'][^\"']*kabel[^\"']*[\"']\\]/gi"
  ],
  "words": [
    "actrc",
    "alexkrechik",
    "amatiasq",
    "bddgen",
    "bunx",
    "configurator",
    "csrfmiddlewaretoken",
    "dbaeumer",
    "deque",
    "Deque",
    "ejazullah",
    "esbenp",
    "gitmoji",
    "gitmojis",
    "kabel",
    "Kbps",
    "lintstagedrc",
    "maxTTFB",
    "optin",
    "signup",
    "sonarjs",
    "TTFB",
    "uitestingplayground",
    "usernamehw",
    "widthxheight",
    "yoavbls"
  ]
}
